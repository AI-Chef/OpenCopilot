{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Celery Workers",
            "type": "python",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "celery_app",
                "worker",
                "-l",
                "debug"
            ],
            "env": {
                "OBJC_DISABLE_INITIALIZE_FORK_SAFETY": "YES",
                "DISABLE_SPRING": "true"
            },
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/../ll-server/.env"
        }
    ]
  }
  

